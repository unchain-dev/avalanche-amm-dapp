    "@openzeppelin/test-helpers": "^0.5.16",
    "@typechain/ethers-v5": "^10.1.0",
    "@typechain/hardhat": "^6.1.2",
    "@types/chai": "^4.2.0",
    "@types/mocha": ">=9.1.0",
    "eslint": "^8.41.0",
    "eslint-plugin-node": "^11.1.0",
    "hardhat": "^2.11.2",
    "hardhat-gas-reporter": "^1.0.8",
    "solidity-coverage": "^0.8.1",
